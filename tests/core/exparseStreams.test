====================================================================================================
Read operator.
====================================================================================================
----------------------------------------------------------------------------------------------------

<< channel

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<Read>
    <Id name="channel"/>
</Read>

----------------------------------------------------------------------------------------------------

n = << channel

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<AssignmentExpression op="=">
    <Id name="n"/>
    <Read>
        <Id name="channel"/>
    </Read>
</AssignmentExpression>

----------------------------------------------------------------------------------------------------

foo(<< channel)

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<CallExpression>
    <Id name="foo"/>
    <arg>
        <Read>
            <Id name="channel"/>
        </Read>
    </arg>
</CallExpression>

====================================================================================================
Write operator.
====================================================================================================
----------------------------------------------------------------------------------------------------

>> message

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<Yield>
    <Id name="message"/>
</Yield>

----------------------------------------------------------------------------------------------------

message >> channel

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<Write>
    <Id name="message"/>
    <Id name="channel"/>
</Write>

----------------------------------------------------------------------------------------------------
# Write expression in block

>> {
    message
}

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<Yield>
    <Id name="message"/>
</Yield>

----------------------------------------------------------------------------------------------------
# Write expression in block

>> {
    message
} where {
    message = 1
}

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<Yield>
    <Block>
        <Id name="message"/>
        <where>
            <AssignmentExpression op="=">
                <Id name="message"/>
                <Integer value="1"/>
            </AssignmentExpression>
        </where>
    </Block>
</Yield>

====================================================================================================
Write all operator.
====================================================================================================
----------------------------------------------------------------------------------------------------
# Write all expression without channel

*>> channel

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<Yield all="true">
    <Id name="channel"/>
</Yield>

----------------------------------------------------------------------------------------------------
# Write all expression without channel

channel *>> channel

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<Write all="true">
    <Id name="channel"/>
    <Id name="channel"/>
</Write>

----------------------------------------------------------------------------------------------------
