====================================================================================================
Write operator.
====================================================================================================
----------------------------------------------------------------------------------------------------

>> message

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<UnaryExpression op=">>">
    <Id name="message"/>
</UnaryExpression>

----------------------------------------------------------------------------------------------------

message >> channel

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<BinaryExpression op=">>">
    <Id name="message"/>
    <Id name="channel"/>
</BinaryExpression>

----------------------------------------------------------------------------------------------------
# Write expression in block

>> {
    message
}

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<UnaryExpression op=">>">
    <Id name="message"/>
</UnaryExpression>

----------------------------------------------------------------------------------------------------
# Write expression in block

>> {
    message
} where {
    message = 1
}

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<UnaryExpression op=">>">
    <Block>
        <Id name="message"/>
        <where>
            <AssignmentExpression op="=">
                <Id name="message"/>
                <Integer value="1"/>
            </AssignmentExpression>
        </where>
    </Block>
</UnaryExpression>

