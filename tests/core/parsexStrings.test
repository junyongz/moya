====================================================================================================
# Basic quoted strings

Strings are enclosed within double quotation marks. Single quoted strings are treated as individual characters, as you will see later.

@string/basic
====================================================================================================
----------------------------------------------------------------------------------------------------

""

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<String value=""/>

----------------------------------------------------------------------------------------------------

"abc"

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<String value="abc"/>

----------------------------------------------------------------------------------------------------
# Leading and trailing whitespace is preserved, as it obviously should be.

"  abc  "

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<String value="  abc  "/>

----------------------------------------------------------------------------------------------------
\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\

foo"bar"

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<CallExpression>
    <Id name="foo"/>
    <Argument>
        <String value="bar"/>
    </Argument>
</CallExpression>

----------------------------------------------------------------------------------------------------

foo"\\-"

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<CallExpression>
    <Id name="foo"/>
    <Argument>
        <String value="\-"/>
    </Argument>
</CallExpression>

----------------------------------------------------------------------------------------------------

foo"abc %def"

- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

<CallExpression>
    <Id name="foo"/>
    <Argument>
        <BinaryExpression op="concat">
            <String value="abc "/>
            <Id name="def"/>
        </BinaryExpression>
    </Argument>
</CallExpression>

----------------------------------------------------------------------------------------------------
