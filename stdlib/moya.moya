
C"char* resizeBuffer(char*, int, int)"

+ resize\T(items:T, size:Int32, oldSize, newSize):T do {
    = resizeBuffer(items, size * oldSize, size * newSize)
}

----------------------------------------------------------------------------------------------------

+ Error {
    + message = ""
    
    + This(msg:String) do {
        message = msg
    }
}

+ OutOfBoundsError : Error {
    + This(msg:String) do {
        message = msg
    }
}

